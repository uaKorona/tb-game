{
  "name": "tb-game",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "buildFront": "cd tbGameSpa && npm run build",
    "start": "node dist/index.js",
    "start-debug": "npm run debug && npm run start",
    "build": "tsc",
    "dev": "nodemon --watch src --exec ts-node index.ts",
    "watch-ts": "tsc -w",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "lint": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uaKorona/tb-game.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uaKorona/tb-game/issues"
  },
  "homepage": "https://github.com/uaKorona/tb-game#readme",
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@angular/cli": "^16.1.0",
    "aws-sdk": "^2.1386.0",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "socket.io": "^4.6.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.4",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.35.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.16",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-node": "^10.7.0"
  }
}
